{
  "version": 2,
  "projects": {
    "front": {
      "projectType": "application",
      "root": "apps/front",
      "sourceRoot": "apps/front/src",
      "prefix": "everteam",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/front",
            "index": "apps/front/src/index.html",
            "main": "apps/front/src/main.ts",
            "polyfills": "apps/front/src/polyfills.ts",
            "tsConfig": "apps/front/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/front/src/favicon.ico",
              "apps/front/src/assets"
            ],
            "styles": [
              "apps/front/src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/saga-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "./node_modules/primeflex/primeflex.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/front/src/environments/environment.ts",
                  "with": "apps/front/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "front:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "front:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "front:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/front/src/**/*.ts",
              "apps/front/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/front/src/test.ts",
            "tsConfig": "apps/front/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js"
          }
        }
      }
    },
    "ui-components": {
      "projectType": "library",
      "root": "libs/ui-components",
      "sourceRoot": "libs/ui-components/src",
      "prefix": "ever-team",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-components/src/**/*.ts",
              "libs/ui-components/src/**/*.html"
            ]
          }
        }
      }
    },
    "colaboradores": {
      "projectType": "library",
      "root": "libs/colaboradores",
      "sourceRoot": "libs/colaboradores/src",
      "prefix": "ever-team",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/colaboradores/src/**/*.ts",
              "libs/colaboradores/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/colaboradores/src/test.ts",
            "tsConfig": "libs/colaboradores/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js"
          }
        }
      }
    },
    "demands": {
      "projectType": "library",
      "root": "libs/demands",
      "sourceRoot": "libs/demands/src",
      "prefix": "ever-team",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/demands/src/**/*.ts",
              "libs/demands/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/demands/src/test.ts",
            "tsConfig": "libs/demands/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js"
          }
        }
      }
    },
    "dashboard": {
      "projectType": "library",
      "root": "libs/dashboard",
      "sourceRoot": "libs/dashboard/src",
      "prefix": "ever-team",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/dashboard/src/**/*.ts",
              "libs/dashboard/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/dashboard/src/test.ts",
            "tsConfig": "libs/dashboard/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "front"
}